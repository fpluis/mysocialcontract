{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint-config-pretty-strict",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "17.0.2"
    }
  },
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "off",
    "no-use-before-define": "off",
    "strict": "off",
    "class-methods-use-this": "off"
  }
}